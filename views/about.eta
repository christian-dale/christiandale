<% layout("main", {metaDescription: "About me", canonical: "/about"}) %>

<link rel="stylesheet" href="/assets/css/timeline.css">

<div class="section-me" id="about">
    <div class="ke-container-lg">
        <div class="ke-grid">
            <div class="ke-col-1">
                <img src="/assets/img/blog/hope.jpg" alt="Picture of Christian Dale" style="border-radius: 5px; width: 100%; object-fit: cover;">
            </div>
            <div class="ke-col-1">
                <h1><%~ App.lang("about:title") %></h1>
                <p style="margin-top: 20px;"><%= App.lang("about:description") %></p>
                <p><%~ App.lang("about:contactmeText") %></p>
                <!--<a href="/contact" class="ke-button ke-btn-default">Contact form</a>-->
                <a href="#section-skills" class="ke-button ke-btn-primary"><%= App.lang("about:btnListOfSkills") %></a>
                <a href="#section-work" class="ke-button ke-btn-primary"><%= App.lang("about:btnMyWork") %></a>
                <a href="#section-timeline" class="ke-button ke-btn-primary"><%= App.lang("about:btnTimeline") %></a>
            </div>
        </div>
    </div>
</div>

<div class="" style="margin: 0;">
<div class="">
<div id="section-skills" class="section-skills" style="padding: 75px 0; background-color: #f5f5f5;">
    <div class="ke-container">
        <h1><%= App.lang("about:listOfSkills") %></h1>
        <p><%= App.lang("about:listOfSkillsText") %></p>

        <h3><%= App.lang("about:listOfSkills") %></h3>

        <p><b>HTML / CSS / Javascript</b></p>
